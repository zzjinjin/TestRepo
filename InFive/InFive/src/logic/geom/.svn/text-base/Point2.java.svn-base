package logic.geom;

import java.util.HashMap;
import java.util.Map;

public class Point2 {

	private static final int range=0;
	int x,y;
	Map<Integer,Integer> id_no=new HashMap<Integer, Integer>();
	
	public boolean isEqual(Point2 p){
		if(Math.abs(x-p.x)<=range && Math.abs(y-p.y)<=range)
			return true;
		else
			return false;
	}
}
